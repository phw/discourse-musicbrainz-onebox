{{#image}}<img src="{{image}}" class="thumbnail"/>{{/image}}

<h3>
  <a href="{{link}}">{{title}}</a>
  {{#disambiguation}}
  <span class="disambiguation">({{disambiguation}})</span>
  {{/disambiguation}}
</h3>

<p>
  {{type}}{{#secondary}} ({{secondary}}){{/secondary}} by {{artist}}{{#date}}, {{date}}{{/date}}
</p>
